@prefix ksa-ft: <https://w3id.org/ksa/feature-types/> .
@prefix landParcels: <https://w3id.org/ksa/feature-types/LandParcels/> .
@prefix landParcels-att: <https://w3id.org/ksa/feature-types/LandParcels/attribute/> .
@prefix nationalAddress: <https://w3id.org/ksa/feature-types/NationalAddress/> .
@prefix ogc-ft: <http://www.opengis.net/def/metamodel/featuretypes/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

landParcels:BuildingConstructionStatusValue a owl:Class,
        skos:Concept ;
    rdfs:label "Building Construction Status Value"@en ;
    skos:broader landParcels:codeLists ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "Building Construction Status Value"@en .

landParcels:BuildingMaterialValue a owl:Class,
        skos:Concept ;
    rdfs:label "Building Material Value"@en ;
    skos:broader landParcels:codeLists ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "Building Material Value"@en .

landParcels:BuildingStatusValue a owl:Class,
        skos:Concept ;
    rdfs:label "Building Status Value"@en ;
    skos:broader landParcels:codeLists ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "Building Status Value"@en .

landParcels:FacilityTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Facility Type Value"@en ;
    skos:broader landParcels:codeLists ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "Facility Type Value"@en .

landParcels:ParcelStatusValue a owl:Class,
        skos:Concept ;
    rdfs:label "Parcel Status Value"@en ;
    skos:broader landParcels:codeLists ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "Parcel Status Value"@en .

landParcels-att:address a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "address" ;
    rdfs:domain landParcels:SpatialUnit ;
    rdfs:range nationalAddress:Address ;
    rdfs:subClassOf landParcels:attribute ;
    skos:broader landParcels:attribute ;
    skos:prefLabel "address" .

landParcels-att:area a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "area"@en ;
    rdfs:domain landParcels:SpatialUnit ;
    rdfs:subClassOf landParcels:attribute ;
    skos:broader landParcels:attribute ;
    skos:definition "The area of the 2D spatial unit."@en ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "area"@en .

landParcels-att:name a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "name"@en ;
    rdfs:domain landParcels:SpatialUnit ;
    rdfs:range xsd:string ;
    rdfs:subClassOf landParcels:attribute ;
    skos:broader landParcels:attribute ;
    skos:definition "Short textual description of the spatial unit - parcel or building."@en ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "name"@en .

landParcels-att:referencePoint a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "referencePoint"@en ;
    rdfs:domain landParcels:SpatialUnit ;
    rdfs:subClassOf landParcels:attribute ;
    skos:broader landParcels:attribute ;
    skos:definition "The coordinates of a point inside the spatial unit."@en ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "referencePoint"@en .

landParcels-att:suId a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "suId"@en ;
    rdfs:domain landParcels:SpatialUnit ;
    rdfs:range <https://w3id.org/ksa/feature-types/BaseTypes/ThematicIdentifier> ;
    rdfs:subClassOf landParcels:attribute ;
    skos:broader landParcels:attribute ;
    skos:definition "Local identifier qualified by registration organization."@en ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "suId"@en .

nationalAddress:Address a owl:Class,
        skos:Concept ;
    rdfs:label "Address"@en ;
    skos:definition "An identification of the fixed location of property by means of a structured composition of geographic names and identifiers."@en ;
    skos:prefLabel "Address"@en .

landParcels:SpatialUnit a owl:Class,
        skos:Concept ;
    rdfs:label "Spatial Unit"@en ;
    ogc-ft:hasProperty landParcels-att:area,
        landParcels-att:name,
        landParcels-att:referencePoint,
        landParcels-att:suId ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/BaseTypes/AbstractFeatureType> ;
    skos:broader <https://w3id.org/ksa/feature-types/BaseTypes/AbstractFeatureType> ;
    skos:definition "Single area (or multiple areas) of land and/or water, or a single volume (or multiple volumes) of space."@en ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "Spatial Unit"@en .

landParcels:codeLists a owl:Class,
        skos:Concept ;
    rdfs:label "Land Parcels Foundation Theme code list or type"@en ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "Land Parcels Foundation Theme code list or type"@en .

landParcels:attribute a owl:Class,
        skos:Concept ;
    rdfs:label "Land Parcels Foundation Theme attribute"@en ;
    skos:inScheme ksa-ft:LandParcels ;
    skos:prefLabel "Land Parcels Foundation Theme attribute"@en .

ksa-ft:LandParcels a skos:ConceptScheme ;
    rdfs:label "Land Parcels Foundation Theme"@en ;
    skos:definition "The Land Parcels Foundation Theme is a continuously updated nationwide information resource, which contains detailed geometry and other attribute information for Land Parcels and Building Footprints. It also enables linking of land registration, ownership and rights information to Land Parcels and Building Footprints. Geometry must be referenced to the Saudi Arabia National Spatial Reference System (SANSRS) and be topologically structured. Each Land Parcel and Building Footprint shall have a unique identifier allowing other Foundation Themes (e.g. Land Use) and applications to reference them."@en ;
    skos:prefLabel "Land Parcels Foundation Theme"@en .

