@prefix ksa-ft: <https://w3id.org/ksa/feature-types/> .
@prefix ogc-ft: <http://www.opengis.net/def/metamodel/featuretypes/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ksa-ft:BuildingConstructionStatusValue a owl:Class,
        skos:Concept ;
    rdfs:label "Building Construction Status Value"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/codeLists> ;
    skos:inScheme ksa-ft:LandParcels .

ksa-ft:BuildingMaterialValue a owl:Class,
        skos:Concept ;
    rdfs:label "Building Material Value"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/codeLists> ;
    skos:inScheme ksa-ft:LandParcels .

ksa-ft:BuildingStatusValue a owl:Class,
        skos:Concept ;
    rdfs:label "Building Status Value"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/codeLists> ;
    skos:inScheme ksa-ft:LandParcels .

ksa-ft:FacilityTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Facility Type Value"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/codeLists> ;
    skos:inScheme ksa-ft:LandParcels .

<https://w3id.org/ksa/feature-types/LandParcels/attribute/address> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "address" ;
    rdfs:domain ksa-ft:SpatialUnit ;
    rdfs:range ksa-ft:Address ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/LandParcels/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/attribute> .

ksa-ft:ParcelStatusValue a owl:Class,
        skos:Concept ;
    rdfs:label "Parcel Status Value"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/codeLists> ;
    skos:inScheme ksa-ft:LandParcels .

ksa-ft:Address a owl:Class,
        skos:Concept ;
    rdfs:label "Address"@en ;
    skos:definition "An identification of the fixed location of property by means of a structured composition of geographic names and identifiers."@en .

<https://w3id.org/ksa/feature-types/LandParcels/attribute/area> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "area"@en ;
    rdfs:domain ksa-ft:SpatialUnit ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/LandParcels/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/attribute> ;
    skos:definition "The area of the 2D spatial unit."@en ;
    skos:inScheme ksa-ft:LandParcels .

<https://w3id.org/ksa/feature-types/LandParcels/attribute/name> a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "name"@en ;
    rdfs:domain ksa-ft:SpatialUnit ;
    rdfs:range xsd:string ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/LandParcels/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/attribute> ;
    skos:definition "Short textual description of the spatial unit - parcel or building."@en ;
    skos:inScheme ksa-ft:LandParcels .

<https://w3id.org/ksa/feature-types/LandParcels/attribute/referencePoint> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "referencePoint"@en ;
    rdfs:domain ksa-ft:SpatialUnit ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/LandParcels/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/attribute> ;
    skos:definition "The coordinates of a point inside the spatial unit."@en ;
    skos:inScheme ksa-ft:LandParcels .

<https://w3id.org/ksa/feature-types/LandParcels/attribute/suId> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "suId"@en ;
    rdfs:domain ksa-ft:SpatialUnit ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/LandParcels/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/LandParcels/attribute> ;
    skos:definition "Local identifier qualified by registration organization."@en ;
    skos:inScheme ksa-ft:LandParcels .

<https://w3id.org/ksa/feature-types/LandParcels/codeLists> a owl:Class,
        skos:Concept ;
    rdfs:label "Land Parcels Foundation Theme code list or type"@en ;
    skos:inScheme ksa-ft:LandParcels .

ksa-ft:SpatialUnit a owl:Class,
        skos:Concept ;
    rdfs:label "Spatial Unit"@en ;
    ogc-ft:hasProperty <https://w3id.org/ksa/feature-types/LandParcels/attribute/area>,
        <https://w3id.org/ksa/feature-types/LandParcels/attribute/name>,
        <https://w3id.org/ksa/feature-types/LandParcels/attribute/referencePoint>,
        <https://w3id.org/ksa/feature-types/LandParcels/attribute/suId> ;
    rdfs:subClassOf ksa-ft:AbstractFeatureType ;
    skos:broader ksa-ft:AbstractFeatureType ;
    skos:definition "Single area (or multiple areas) of land and/or water, or a single volume (or multiple volumes) of space."@en ;
    skos:inScheme ksa-ft:LandParcels .

<https://w3id.org/ksa/feature-types/LandParcels/attribute> a owl:Class,
        skos:Concept ;
    rdfs:label "Land Parcels Foundation Theme attribute"@en ;
    skos:inScheme ksa-ft:LandParcels .

ksa-ft:LandParcels a skos:ConceptScheme ;
    rdfs:label "Land Parcels Foundation Theme"@en ;
    skos:definition "The Land Parcels Foundation Theme is a continuously updated nationwide information resource, which contains detailed geometry and other attribute information for Land Parcels and Building Footprints. It also enables linking of land registration, ownership and rights information to Land Parcels and Building Footprints. Geometry must be referenced to the Saudi Arabia National Spatial Reference System (SANSRS) and be topologically structured. Each Land Parcel and Building Footprint shall have a unique identifier allowing other Foundation Themes (e.g. Land Use) and applications to reference them."@en .

