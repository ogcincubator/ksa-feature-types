@prefix ksa-ft: <https://w3id.org/ksa/feature-types/> .
@prefix ogc-ft: <http://www.opengis.net/def/metamodel/featuretypes/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix physInfra: <https://w3id.org/ksa/feature-types/PhysicalInfrastructureTheme/> .
@prefix physInfra-att: <https://w3id.org/ksa/feature-types/PhysicalInfrastructureTheme/attribute/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

physInfra:CopperCable a owl:Class,
        skos:Concept ;
    rdfs:label "CopperCable"@en ;
    rdfs:subClassOf ksa-ft:Cable ;
    skos:broader ksa-ft:Cable ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "CopperCable"@en .

physInfra:DataCenter a owl:Class,
        skos:Concept ;
    rdfs:label "Communications Building"@en ;
    rdfs:subClassOf ksa-ft:CommsBuilding ;
    skos:broader ksa-ft:CommsBuilding ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "Communications Building"@en .

physInfra:FrequencyBand a owl:Class,
        skos:Concept ;
    rdfs:label "FrequencyBand"@en ;
    skos:broader physInfra:codeLists ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "FrequencyBand"@en .

physInfra:WirelessTechnology a owl:Class,
        skos:Concept ;
    rdfs:label "WirelessTechnology"@en ;
    skos:broader physInfra:codeLists ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "WirelessTechnology"@en .

physInfra-att:cellSite a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "cellSite" ;
    rdfs:domain physInfra:Antenna ;
    rdfs:range physInfra:CellSite ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:prefLabel "cellSite" .

physInfra-att:landingStation a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "landingStation" ;
    rdfs:domain physInfra:BeachManhole ;
    rdfs:range physInfra:LandingStation ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:prefLabel "landingStation" .

physInfra-att:segments a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "segments" ;
    rdfs:domain physInfra:Cable,
        physInfra:Pipeline ;
    rdfs:range physInfra:LinkSequence ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:prefLabel "segments" .

physInfra-att:tower a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "tower" ;
    rdfs:domain physInfra:Antenna ;
    rdfs:range physInfra:Tower ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:prefLabel "tower" .

physInfra:AssetAreaFootprint a owl:Class,
        skos:Concept ;
    rdfs:label "AssetAreaFootprint"@en ;
    ogc-ft:hasProperty physInfra-att:geometry ;
    skos:definition "A description of the area occupied by an asset, allowing a straditional network model to have indicative location to act as point geometry for a network node."@en ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "AssetAreaFootprint"@en .

physInfra:BeachManhole a owl:Class,
        skos:Concept ;
    rdfs:label "BeachManhole"@en ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "BeachManhole"@en .

physInfra:CellSite a owl:Class,
        skos:Concept ;
    rdfs:label "CellSite"@en ;
    skos:prefLabel "CellSite"@en .

physInfra:LandingStation a owl:Class,
        skos:Concept ;
    rdfs:label "LandingStation"@en ;
    skos:definition "<font color=\"#000061\"></font>"@en ;
    skos:prefLabel "LandingStation"@en .

physInfra:LinkSequence a owl:Class,
        skos:Concept ;
    rdfs:label "Link Sequence"@en ;
    skos:definition "A network element which represents a continuous path in the network without any branches. The element has a defined beginning and end and every position on the link sequence is identifiable with one single parameter such as length."@en ;
    skos:prefLabel "Link Sequence"@en .

physInfra:Tower a owl:Class,
        skos:Concept ;
    rdfs:label "Tower"@en ;
    skos:prefLabel "Tower"@en .

physInfra-att:azimuth a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "azimuth"@en ;
    rdfs:domain physInfra:Antenna ;
    rdfs:range xsd:double ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "azimuth"@en .

physInfra-att:beamWidth a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "beamWidth"@en ;
    rdfs:domain physInfra:Antenna ;
    rdfs:range xsd:double ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "beamWidth"@en .

physInfra-att:electricalTilt a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "electricalTilt"@en ;
    rdfs:domain physInfra:Antenna ;
    rdfs:range xsd:double ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "electricalTilt"@en .

physInfra-att:mechanicalTilt a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "mechanicalTilt"@en ;
    rdfs:domain physInfra:Antenna ;
    rdfs:range xsd:double ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "mechanicalTilt"@en .

physInfra-att:mountHeight a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "mountHeight"@en ;
    rdfs:domain physInfra:Antenna ;
    rdfs:range xsd:double ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "mountHeight"@en .

physInfra-att:voltageClass a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "voltageClass"@en ;
    rdfs:domain physInfra:Cable ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "voltageClass"@en .

physInfra:Pipeline a owl:Class,
        skos:Concept ;
    rdfs:label "Pipeline"@en ;
    ogc-ft:hasProperty physInfra-att:geometry ;
    skos:definition "A named Entity that may link a single pathway between one or more network nodes in a defined sequence."@en ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "Pipeline"@en .

physInfra:codeLists a owl:Class,
        skos:Concept ;
    rdfs:label "Physical Infrastructure Theme code list or type"@en ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "Physical Infrastructure Theme code list or type"@en .

physInfra:Cable a owl:Class,
        skos:Concept ;
    rdfs:label "Cable"@en ;
    ogc-ft:hasProperty physInfra-att:geometry,
        physInfra-att:voltageClass ;
    skos:definition "A named Entity that may link a single pathway between one or more network nodes in a defined sequence."@en ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "Cable"@en .

physInfra-att:geometry a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "geometry"@en ;
    rdfs:domain physInfra:AssetAreaFootprint,
        physInfra:Cable,
        physInfra:Pipeline ;
    rdfs:subClassOf physInfra:attribute ;
    skos:broader physInfra:attribute ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "geometry"@en .

physInfra:Antenna a owl:Class,
        skos:Concept ;
    rdfs:label "Antenna"@en ;
    ogc-ft:hasProperty physInfra-att:azimuth,
        physInfra-att:beamWidth,
        physInfra-att:electricalTilt,
        physInfra-att:mechanicalTilt,
        physInfra-att:mountHeight ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "Antenna"@en .

ksa-ft:PhysicalInfrastructureTheme a skos:ConceptScheme ;
    rdfs:label "Physical Infrastructure Theme"@en ;
    skos:prefLabel "Physical Infrastructure Theme"@en .

physInfra:attribute a owl:Class,
        skos:Concept ;
    rdfs:label "Physical Infrastructure Theme attribute"@en ;
    skos:inScheme ksa-ft:PhysicalInfrastructureTheme ;
    skos:prefLabel "Physical Infrastructure Theme attribute"@en .

