<https://w3id.org/ksa/feature-types/Transport/attribute/node> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "node" ;
    rdfs:domain ksa-ft:MetroStation ;
    rdfs:range ksa-ft:MetroNode ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> .

ksa-ft:VehicleTypeValue a owl:Class,
