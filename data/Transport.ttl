    rdfs:label "Vehicle Type Value"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/codeLists> ;
    skos:definition "Possible types of vehicles."@en ;
    skos:inScheme ksa-ft:Transport .

ksa-ft:VerticalPositionTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Vertical Position Type Value"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/codeLists> ;
    skos:definition "A coded domain value denoting a relative vertical position of the transport element."@en ;
    skos:inScheme ksa-ft:Transport .

ksa-ft:MetroNode a owl:Class,
        skos:Concept ;
    rdfs:label "Metro Node"@en ;
    skos:definition "A point feature which represents a significant point along the metro network or defines an intersection of metro tracks used to describe its connectivity."@en .

ksa-ft:PathwayTypeValue a owl:Class,
