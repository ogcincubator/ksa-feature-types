transport:Bollard a owl:Class,
        skos:Concept ;
    rdfs:label "Bollard"@en ;
    rdfs:subClassOf transport:RoadFurniture ;
    skos:broader transport:RoadFurniture ;
    skos:definition "A short post used to prevent traffic from entering an area."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Bollard"@en .

transport:BusRouteLink a owl:Class,
        skos:Concept ;
    rdfs:label "Bus Route Link"@en ;
    rdfs:subClassOf transport:TransportLink ;
    skos:broader transport:TransportLink ;
    skos:definition "A linear feature that describes the geometry and connectivity of a bus network between two points in the network."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Bus Route Link"@en .

transport:BusRouteLinkSequence a owl:Class,
        skos:Concept ;
    rdfs:label "Bus Route Link Sequence"@en ;
    rdfs:subClassOf transport:TransportLinkSequence ;
    skos:broader transport:TransportLinkSequence ;
    skos:definition "A linear feature composed of an ordered collection of bus route links, which represents a continuous path in a bus route network without any branches. The element has a defined beginning and end and every position on the bus route link sequence."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Bus Route Link Sequence"@en .

transport:CarriagewayNode a owl:Class,
        skos:Concept ;
    rdfs:label "Carriageway Node"@en ;
    rdfs:subClassOf transport:TransportNode ;
    skos:broader transport:TransportNode ;
    skos:definition "A point feature that is used to represent connectivity between two carriageway links."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Carriageway Node"@en .

transport:FuelStation a owl:Class,
        skos:Concept ;
    rdfs:label "Fuel Station"@en ;
    rdfs:subClassOf transport:TransportProperty ;
    skos:broader transport:TransportProperty ;
    skos:definition "An establishment beside a road selling fuel for motor vehicles, may also be called a petrol or gas station."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Fuel Station"@en .

transport:ITSScreen a owl:Class,
        skos:Concept ;
    rdfs:label "ITS Screen"@en ;
    rdfs:subClassOf transport:RoadFurniture ;
    skos:broader transport:RoadFurniture ;
    skos:definition "A screen used to show how different networks of transport are linked with each other."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "ITS Screen"@en .

transport:MetroLinkSequence a owl:Class,
        skos:Concept ;
    rdfs:label "Metro Link Sequence"@en ;
    rdfs:subClassOf transport:TransportLinkSequence ;
    skos:broader transport:TransportLinkSequence ;
    skos:definition "A linear feature composed of an ordered collection of metro links, which represents a continuous path in a metro network without any branches. The element has a defined beginning and end and every position on the metro link sequence."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Metro Link Sequence"@en .

transport:MetroStationArea a owl:Class,
        skos:Concept ;
    rdfs:label "Metro Station Area"@en ;
    rdfs:subClassOf transport:TransportArea ;
    skos:broader transport:TransportArea ;
    skos:definition "A metro station is a type of railway station that is part of a metro, rapid transit system, subway or underground railway line or railway network. Geometry representation of metro station area is an extent."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Metro Station Area"@en .

transport:ParkingPayStation a owl:Class,
        skos:Concept ;
    rdfs:label "Parking Pay Station"@en ;
    rdfs:subClassOf transport:RoadFurniture ;
    skos:broader transport:RoadFurniture ;
    skos:definition "A device used to collect money in exchange for the right to park a vehicle in a particular place for a limited amount of time, includes parking meters."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Parking Pay Station"@en .

transport:RailwayLinkSequence a owl:Class,
        skos:Concept ;
    rdfs:label "Railway Link Sequence"@en ;
    rdfs:subClassOf transport:TransportLinkSequence ;
    skos:broader transport:TransportLinkSequence ;
    skos:definition "A linear feature composed of an ordered collection of railway links, which represents a continuous path in a railway network without any branches. The element has a defined beginning and end and every position on the railway link sequence is identifiable with one single parameter such as length. It describes an element of the railway network, characterized by one or more thematical identifiers and/or properties."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Railway Link Sequence"@en .

transport:RailwayStationArea a owl:Class,
        skos:Concept ;
    rdfs:label "Railway Station Area"@en ;
    rdfs:subClassOf transport:TransportArea ;
    skos:broader transport:TransportArea ;
    skos:definition "An area feature which is used to represent the topographical limits of the facilities of a railway station (buildings, railway yards, installations and equipment) devoted to carry out railway station operations."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Railway Station Area"@en .

transport:RoadLaneNode a owl:Class,
        skos:Concept ;
    rdfs:label "Road Lane Node"@en ;
    rdfs:subClassOf transport:TransportNode ;
    skos:broader transport:TransportNode ;
    skos:definition "A point that is used to represent connectivity between two road lane links."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Road Lane Node"@en .

transport:RoadLinkSequence a owl:Class,
        skos:Concept ;
    rdfs:label "Road Link Sequence"@en ;
    rdfs:subClassOf transport:TransportLinkSequence ;
    skos:broader transport:TransportLinkSequence ;
    skos:definition "A line composed of an ordered collection of road links, which represents a continuous path in a road network without any branches. The element has a defined beginning and end and every position on the road link sequence."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Road Link Sequence"@en .

transport:TaxiStation a owl:Class,
