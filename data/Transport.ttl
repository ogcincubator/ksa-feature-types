ksa-ft:StatusTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Status Type Value"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/codeLists> ;
    skos:definition "A coded domain value denoting a status of a transport element (e.g. projected, underConstruction)"@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute/alternativeName> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "alternativeName"@en ;
    rdfs:domain ksa-ft:Pathway ;
    rdfs:range ksa-ft:GeographicalName ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:definition "Alternative name of a pathway, as assigned by the responsible authority."@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute/centerline> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "centerline"@en ;
    rdfs:domain ksa-ft:Pathway ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:definition "Geometry of the pathway."@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute/class> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "class"@en ;
    rdfs:domain ksa-ft:Pathway ;
    rdfs:range ksa-ft:PathwayTypeValue ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:definition "Type of pathway for pedestrian or wheelchair."@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute/description> a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "description"@en ;
    rdfs:domain ksa-ft:AbstractTransportType ;
    rdfs:range xsd:string ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:definition "Description of the transport element."@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute/height> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "height"@en ;
    rdfs:domain ksa-ft:ControlTower ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:definition "Height of the control tower."@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute/name> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "name"@en ;
    rdfs:domain ksa-ft:AbstractTransportType ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:definition "Name of the transport element."@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute/status> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "status"@en ;
    rdfs:domain ksa-ft:AbstractTransportType ;
    rdfs:range ksa-ft:StatusTypeValue ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:definition "Status of the transport element (e.g. projected, under construction)."@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute/width> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "width"@en ;
    rdfs:domain ksa-ft:Pathway ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:definition "Width of the pathway."@en ;
    skos:inScheme ksa-ft:Transport .

ksa-ft:ControlTower a owl:Class,
        skos:Concept ;
    rdfs:label "Control Tower"@en ;
    ogc-ft:hasProperty <https://w3id.org/ksa/feature-types/Transport/attribute/centerpoint>,
        <https://w3id.org/ksa/feature-types/Transport/attribute/height> ;
    rdfs:subClassOf ksa-ft:AbstractTransportType ;
    skos:broader ksa-ft:AbstractTransportType ;
    skos:definition "A tall building at an airport from which the movements of air traffic are controlled."@en ;
    skos:inScheme ksa-ft:Transport .

ksa-ft:MetroStation a owl:Class,
        skos:Concept ;
    rdfs:label "Metro Station"@en ;
    ogc-ft:hasProperty <https://w3id.org/ksa/feature-types/Transport/attribute/centerpoint> ;
    rdfs:subClassOf ksa-ft:AbstractTransportType ;
    skos:broader ksa-ft:AbstractTransportType ;
    skos:definition "A metro station is a type of railway station that is part of a metro, rapid transit system, subway or underground railway line or railway network. Geometry representation of metro station is a center point."@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute/centerpoint> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "centerpoint"@en ;
    rdfs:domain ksa-ft:ControlTower,
        ksa-ft:MetroStation ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Transport/attribute> ;
    skos:definition "Geometry representation of metro station is a center point."@en,
        "Location of the control tower."@en ;
    skos:inScheme ksa-ft:Transport .

ksa-ft:Pathway a owl:Class,
        skos:Concept ;
    rdfs:label "Pathway"@en ;
    ogc-ft:hasProperty <https://w3id.org/ksa/feature-types/Transport/attribute/alternativeName>,
        <https://w3id.org/ksa/feature-types/Transport/attribute/centerline>,
        <https://w3id.org/ksa/feature-types/Transport/attribute/class>,
        <https://w3id.org/ksa/feature-types/Transport/attribute/width> ;
    rdfs:subClassOf ksa-ft:AbstractTransportType ;
    skos:broader ksa-ft:AbstractTransportType ;
    skos:definition "A way beaten, formed, or trodden by the feet of persons or animals."@en ;
    skos:inScheme ksa-ft:Transport .

ksa-ft:AbstractTransportType a owl:Class,
        skos:Concept ;
    rdfs:label "Abstract Transport Type"@en ;
    ogc-ft:hasProperty <https://w3id.org/ksa/feature-types/Transport/attribute/description>,
        <https://w3id.org/ksa/feature-types/Transport/attribute/name>,
        <https://w3id.org/ksa/feature-types/Transport/attribute/status> ;
    rdfs:subClassOf ksa-ft:AbstractFeatureType ;
    skos:broader ksa-ft:AbstractFeatureType ;
    skos:definition "Abstract parent class for feature types that holds common attributes."@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/attribute> a owl:Class,
        skos:Concept ;
    rdfs:label "Transport Foundation Theme attribute"@en ;
    skos:inScheme ksa-ft:Transport .

<https://w3id.org/ksa/feature-types/Transport/codeLists> a owl:Class,
        skos:Concept ;
    rdfs:label "Transport Foundation Theme code list or type"@en ;
    skos:inScheme ksa-ft:Transport .

ksa-ft:Transport a skos:ConceptScheme ;
    rdfs:label "Transport Foundation Theme"@en ;
    skos:definition "The Transport Foundation Theme is a suite of road, rail, air and maritime transportation networks and related infrastructure, including connections between the different networks."@en .

