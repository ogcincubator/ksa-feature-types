transport:ControlTower a owl:Class,
        skos:Concept ;
    rdfs:label "Control Tower"@en ;
    ogc-ft:hasProperty transport-att:centerpoint,
        transport-att:height ;
    rdfs:subClassOf ksa-ft:AbstractTransportType ;
    skos:broader ksa-ft:AbstractTransportType ;
    skos:definition "A tall building at an airport from which the movements of air traffic are controlled."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Control Tower"@en .

transport:MetroStation a owl:Class,
        skos:Concept ;
    rdfs:label "Metro Station"@en ;
    ogc-ft:hasProperty transport-att:centerpoint ;
    rdfs:subClassOf ksa-ft:AbstractTransportType ;
    skos:broader ksa-ft:AbstractTransportType ;
    skos:definition "A metro station is a type of railway station that is part of a metro, rapid transit system, subway or underground railway line or railway network. Geometry representation of metro station is a center point."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Metro Station"@en .

transport-att:centerpoint a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "centerpoint"@en ;
    rdfs:domain transport:ControlTower,
        transport:MetroStation ;
    rdfs:subClassOf transport:attribute ;
    skos:broader transport:attribute ;
    skos:definition "Geometry representation of metro station is a center point."@en,
        "Location of the control tower."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "centerpoint"@en .

transport:AbstractTransportType a owl:Class,
        skos:Concept ;
    rdfs:label "Abstract Transport Type"@en ;
    ogc-ft:hasProperty transport-att:description,
        transport-att:name,
        transport-att:status ;
    rdfs:subClassOf ksa-ft:AbstractFeatureType ;
    skos:broader ksa-ft:AbstractFeatureType ;
    skos:definition "Abstract parent class for feature types that holds common attributes."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Abstract Transport Type"@en .

transport:Pathway a owl:Class,
        skos:Concept ;
    rdfs:label "Pathway"@en ;
    ogc-ft:hasProperty transport-att:alternativeName,
        transport-att:centerline,
        transport-att:class,
        transport-att:width ;
    rdfs:subClassOf ksa-ft:AbstractTransportType ;
    skos:broader ksa-ft:AbstractTransportType ;
    skos:definition "A way beaten, formed, or trodden by the feet of persons or animals."@en ;
    skos:inScheme ksa-ft:Transport ;
    skos:prefLabel "Pathway"@en .

transport:attribute a owl:Class,
