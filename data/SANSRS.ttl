        sansrs-att:absoluteGravityCalibrationLinePointID,
        sansrs-att:descriptionOfLocation,
        sansrs-att:ellipsoidalHeight,
        sansrs-att:lastMaintenanceDate,
        sansrs-att:maintenanceUnit,
        sansrs-att:monumentCondition,
        sansrs-att:monumentInstallationDate,
        sansrs-att:monumentType,
        sansrs-att:name,
        sansrs-att:orthometricHeight,
        sansrs-att:position,
        sansrs-att:siteLog,
        sansrs-att:sitePicture,
        sansrs-att:type ;
    rdfs:subClassOf ksa-ft:AbstractFeatureType ;
    skos:broader ksa-ft:AbstractFeatureType ;
    skos:definition "A point on the earth's surface where the acceleration of gravity has been measured relative to other points in a sequence or 'calibration line'."@en ;
    skos:inScheme ksa-ft:SANSRS ;
    skos:prefLabel "Absolute Gravity Calibration Line Point"@en .

