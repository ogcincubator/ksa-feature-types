@prefix elevation: <https://w3id.org/ksa/feature-types/Elevation/> .
@prefix elevation-att: <https://w3id.org/ksa/feature-types/Elevation/attribute/> .
@prefix geology: <https://w3id.org/ksa/feature-types/Geology/> .
@prefix ksa-ft: <https://w3id.org/ksa/feature-types/> .
@prefix ogc-ft: <http://www.opengis.net/def/metamodel/featuretypes/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix transport: <https://w3id.org/ksa/feature-types/Transport/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

elevation:BathymetricElevationGridCoverageAggregation a owl:Class,
        skos:Concept ;
    rdfs:label "Elevation Grid Coverage Aggregation"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "Geometrical characteristics of the elevation grid coverage aggregation."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Elevation Grid Coverage Aggregation"@en .

elevation:LandElevationGridCoverage a owl:Class,
        skos:Concept ;
    rdfs:label "Land Elevation Grid Coverage"@en ;
    rdfs:subClassOf elevation:ElevationGridCoverage ;
    skos:broader elevation:ElevationGridCoverage ;
    skos:definition "Elevation grid for land elevation.  The value of the grid cells is height which is measured upwards."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Land Elevation Grid Coverage"@en .

elevation:LandElevationGridCoverageAggregation a owl:Class,
        skos:Concept ;
    rdfs:label "Elevation Grid Coverage Aggregation"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "Geometrical characteristics of the elevation grid coverage aggregation."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Elevation Grid Coverage Aggregation"@en .

elevation:LandElevationTIN a owl:Class,
        skos:Concept ;
    rdfs:label "Land Elevation TIN"@en ;
    rdfs:subClassOf elevation:ElevationTIN ;
    skos:broader elevation:ElevationTIN ;
    skos:definition "Elevation TIN describing land elevation which depicts heights (measured upwards)."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Land Elevation TIN"@en .

elevation:SurveyTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Survey Type"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Type of the survey conformant with IHO S-101. 

NOTE S-101 acronym is SURTYP."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Survey Type"@en .

elevation-att:CRS a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "CRS" ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range elevation:CRS ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:prefLabel "CRS" .

elevation-att:object a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "object" ;
    rdfs:domain elevation:BathymetricBreaklineCollection,
        elevation:BathymetricElevationContourLineCollection,
        elevation:BathymetricIsolatedAreaCollection,
        elevation:BathymetricVoidAreaCollection,
        elevation:DepthAreaCollection,
        elevation:LandBreakLineCollection,
        elevation:LandElevationContourLineCollection,
        elevation:LandIsolatedAreaCollection,
        elevation:LandSpotElevationCollection,
        elevation:LandVoidAreaCollection,
        elevation:SoundingCollection ;
    rdfs:range elevation:BathymetricBreakLine,
        elevation:BathymetricElevationContourLine,
        elevation:BathymetricIsolatedArea,
        elevation:BathymetricVoidArea,
        elevation:DepthArea,
        elevation:LandBreakLine,
        elevation:LandElevationContourLine,
        elevation:LandIsolatedArea,
        elevation:LandSpotElevation,
        elevation:LandVoidArea,
        elevation:Sounding ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:prefLabel "object" .

elevation:AcquisitionMethodValue a owl:Class,
        skos:Concept ;
    rdfs:label "Acquisition Method"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """List of methods of acquisition of the points in the point cloud.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Acquisition Method"@en .

elevation:BathymetricElevationGridCoverage a owl:Class,
        skos:Concept ;
    rdfs:label "Bathymetric Elevation Grid Coverage"@en ;
    ogc-ft:hasProperty elevation-att:localDepthDatum ;
    rdfs:subClassOf elevation:ElevationGridCoverage ;
    skos:broader elevation:ElevationGridCoverage ;
    skos:definition "Elevation grid for bathymetric elevation.  The value of the grid cells is depth which is measured upwards."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Bathymetric Elevation Grid Coverage"@en .

elevation:BathymetricElevationTIN a owl:Class,
        skos:Concept ;
    rdfs:label "Bathymetric Elevation TIN"@en ;
    ogc-ft:hasProperty elevation-att:localDepthDatum ;
    rdfs:subClassOf elevation:ElevationTIN ;
    skos:broader elevation:ElevationTIN ;
    skos:definition "Elevation TIN describing bathymetric elevation which depicts depth (measured downwards)."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Bathymetric Elevation TIN"@en .

elevation:BathymetricSpotElevationClassValue a owl:Class,
        skos:Concept ;
    rdfs:label "BathymetricSpotElevationClassValue"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Possible classification values for bathymetric spot elevations based on Topo-Bathy LAS Domain Profile.

NOTE These classes are proposed regardless of the acquisition method of the spot elevation. They correspond to the Topo-Bathy LAS Domain Profile point classes."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "BathymetricSpotElevationClassValue"@en .

elevation:BreakLineTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Break Line Type"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "List of possible type values for break lines based on the physical characteristics of the break line [in the elevation surface]."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Break Line Type"@en .

elevation:CRS a owl:Class,
        skos:Concept ;
    rdfs:label "CRS"@en ;
    skos:definition "coordinate reference system which is usually single but may be compound"@en ;
    skos:prefLabel "CRS"@en .

elevation:CRSRepresentationInLASFileValue a owl:Class,
        skos:Concept ;
    rdfs:label "CRS Representation In LAS File"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Representation of the Coordinate Reference System as it is in the LAS file.

NOTE 1 In LAS files versions 1.0-1.3 the CRS was represented as GeoTIFF. In version 1.4 the CRS can be represent as GeoTIFF or WKT (Well Known Text).

NOTE 2 Point Record Formats 0-5 can use either GeoTIFF of WKT. Point Record Formats 6-10 must use WKT.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "CRS Representation In LAS File"@en .

elevation:ChartDatum a owl:Class,
        skos:Concept ;
    rdfs:label "Chart Datum"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "Local vertical coordinate reference system (Coordinate Reference System) which is used to refer and portray depth measurements as property values. This data type is intended to describe the main characteristics of a vertical Coordinate Reference System for measurement of depths in cases where this information is rarely or not available through a Coordinate Reference System registry (for example, like EPSG). Examples of a local vertical Coordinate Reference System include local datums for referring depths in lakes and navigable rivers."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Chart Datum"@en .

elevation:ContourLineTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Contour Line Type"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "List of possible categories of contour lines based on the equidistance parameter of the data set."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Contour Line Type"@en .

elevation:CoordinateComputationalData a owl:Class,
        skos:Concept ;
    rdfs:label "Coordinate Computational Data"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "Encapsulation of data used to compute points coordinates in specified Coordinate Reference System when points are given in raw form of integers only.  For further explanation see ASPRS LAS file specification 1.4 - R15."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Coordinate Computational Data"@en .

elevation:DepthMeasurementQualityValue a owl:Class,
        skos:Concept ;
    rdfs:label "Depth Mesurement Quality"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Quality of depth measurement conformant with IHO s-57. 

NOTE S-57 acronym is QUASOU."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Depth Mesurement Quality"@en .

elevation:DomainProfile a owl:Class,
        skos:Concept ;
    rdfs:label "Domain Profile"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "Domain profile of LAS file."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Domain Profile"@en .

elevation:ExtremalUnscaledCoordinates a owl:Class,
        skos:Concept ;
    rdfs:label "Extremal Unscaled Coordinates"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "The maximum and minimum data fields are the actual unscaled extents of the LAS point file data, specified in the coordinate system of the LAS data.  SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Extremal Unscaled Coordinates"@en .

elevation:GPSTimeTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "GPS Time Type"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Type of the GPS time used in the point records.

NOTE Available values hall be conformant with ASPRS LAS file specification.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "GPS Time Type"@en .

elevation:GeneratingSoftwareValue a owl:Class,
        skos:Concept ;
    rdfs:label "Generating Software Value"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Specification which generating software package and version was used during LAS file creation.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Generating Software Value"@en .

elevation:LASPointDataRecordFormatValue a owl:Class,
        skos:Concept ;
    rdfs:label "LAS point Data Record Format"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Record format associated with points within this point cloud. 

NOTE 1 Point Data Record Format specifies set of attributes that single point holds. The list of available formats is maintained by American Society for Photogrammetry and Remote Sensing in the LAS Specification document.

NOTE 2 In the LAS Specification 1.4 document Point Data Record Formats are indexed for 0 (zero) to 10. In the newer versions of LAS Specification this range can change.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "LAS point Data Record Format"@en .

elevation:LandSpotElevationClassValue a owl:Class,
        skos:Concept ;
    rdfs:label "Spot Elevation Class"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Possible classification values for land spot elevations based on the LAS specification maintained by the American Society for Photogrammetry and Remote Sensing (ASPRS).

NOTE These classes are proposed regardless of the acquisition method of the spot elevation. They correspond to the ASPRS Standard LIDAR Point Classes."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Spot Elevation Class"@en .

elevation:SoundingExpositionValue a owl:Class,
        skos:Concept ;
    rdfs:label "Sounding Exposition Value"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Sounding exposition conformant with IHO s-57. 

NOTE S-57 acronym is EXPSOU."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Sounding Exposition Value"@en .

elevation:SoundingStatusValue a owl:Class,
        skos:Concept ;
    rdfs:label "SoundingStatus"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Sounding status conformant with IHO S-101. 

NOTE S-57 acronym is STATUS."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "SoundingStatus"@en .

elevation:SpotElevationTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Spot Elevation Type"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "Possible values for spot elevation points that describe a singularity of the surface."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Spot Elevation Type"@en .

elevation:SurfaceTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Surface Type"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Closed codeList type which determines the elevation surface with regard to its relative adherence to the Earth's bare surface.

EXAMPLE DTM and DSM."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Surface Type"@en .

elevation:SystemIdentifierValue a owl:Class,
        skos:Concept ;
    rdfs:label "SystemIdentifierValue"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Information about what hardware or what method were used to create points.

NOTE 1 The version 1.0 specification assumed that LAS files are exclusively generated as a result of collection by a hardware sensor. Subsequent versions recognize that files often result from extraction, merging, or modifying existing data files.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "SystemIdentifierValue"@en .

elevation:TechniqueOfSoundingMeasurementValue a owl:Class,
        skos:Concept ;
    rdfs:label "TechniqueOfSoundingMeasurement"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Technique of sounding measurement corresponding to IHO s-57 and IHO S-101.

NOTE S-57 and S-101 acronym is TECSOU."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "TechniqueOfSoundingMeasurement"@en .

elevation:WDPStorageTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "WDP Storage Type Value"@en ;
    skos:broader elevation:codeLists ;
    skos:definition """Type of storage of Waveform Data Package.

EXAMPLE 1 Internal in the LAS file (deprecated in LAS 1.4)

EXAMPLE 2 External in the separate (to LAS file) WDP file that has the same name as corresponding LAS file.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "WDP Storage Type Value"@en .

elevation:WaveformPacketDescriptor a owl:Class,
        skos:Concept ;
    rdfs:label "Waveform Packet Descriptor"@en ;
    skos:broader elevation:codeLists ;
    skos:definition "Information describing the configuration of the waveform packets.  Source: ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Waveform Packet Descriptor"@en .

elevation-att:CRSRepresentation a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "CRSRepresentation"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range elevation:CRSRepresentationInLASFileValue ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Representation of the Coordinate Reference System as it is in the LAS file.

NOTE 1 In LAS files versions 1.0-1.3 the CRS was represented as GeoTIFF. In version 1.4 the CRS can be represent as GeoTIFF or WKT (Well Known Text).

NOTE 2 Point Record Formats 0-5 can use either GeoTIFF of WKT. Point Record Formats 6-10 must use WKT."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "CRSRepresentation"@en .

elevation-att:GPSTimeType a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "GPSTimeType"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range elevation:GPSTimeTypeValue ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "The meaning of GPS Time in the point records."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "GPSTimeType"@en .

elevation-att:LASFile a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "LASFile"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Identifier of LAS file contaninig data for this point cloud."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "LASFile"@en .

elevation-att:LASFileSpecificationVersion a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "LASFileSpecificationVersion"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Version of the LAS file specification.

EXAMPLE 1: 1.2

EXAMPLE 2: 1.4"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "LASFileSpecificationVersion"@en .

elevation-att:WDPFile a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "WDPFile"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "WDP file contaninig asociated with LAS file."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "WDPFile"@en .

elevation-att:WDPStorageType a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "WDPStorageType"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range elevation:WDPStorageTypeValue ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Type of storage of Waveform Data Package.

EXAMPLE 1 Internal in the LAS file (deprecated in LAS 1.4)

EXAMPLE 2 External in the separate (to LAS file) WDP file that has the same name as corresponding LAS file."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "WDPStorageType"@en .

elevation-att:acquisitionMethod a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "acquisitionMethod"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range elevation:AcquisitionMethodValue ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Method of acquisition of the points in the point cloud."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "acquisitionMethod"@en .

elevation-att:averageAltitude a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "averageAltitude"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Average flying altitude above the ground."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "averageAltitude"@en .

elevation-att:averagePositionalAccuracy a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "averagePositionalAccuracy"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Average positional accuracy."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "averagePositionalAccuracy"@en .

elevation-att:averageVerticalAccuracy a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "averageVerticalAccuracy"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Average vertical accuracy of points within point cloud."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "averageVerticalAccuracy"@en .

elevation-att:contourLineType a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "contourLineType"@en ;
    rdfs:domain elevation:GenericContourLine ;
    rdfs:range elevation:ContourLineTypeValue ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """The type of contour line with regard to the normal contour vertical interval (if any).

NOTE This attribute is often applicable to contour lines describing the relief of a land-based area. For depth contours describing the floor of a water body is not always applicable."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "contourLineType"@en .

elevation-att:coordinateComputationalData a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "coordinateComputationalData"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range elevation:CoordinateComputationalData ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Encapsulation of data used to compute points coordinates in specified CRS when points are given in raw form of integers only.

NOTE For further explanation see ASPRS LAS file specification 1.4 - R15."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "coordinateComputationalData"@en .

elevation-att:domainProfile a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "domainProfile"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range elevation:DomainProfile ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Domain profile of LAS file associated with this point cloud."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "domainProfile"@en .

elevation-att:downRight a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "downRight"@en ;
    rdfs:domain elevation:GenericContourLine ;
    rdfs:range xsd:boolean ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Property indicating that the contour line spatial object is digitized in a way that the height of the elevation surface is lower at the right side of the line.

NOTE 1 In the case of land contour lines, when the downRight attribute is 'true' this indicates that the height of the surface is lower at the right side of the line.

NOTE 2 In the case of bathymetry contours, when the downRight attribute is 'true' this indicates that the depth of the surface is higher at the right side of the line."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "downRight"@en .

elevation-att:extent a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "extent"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Extent of the spatiotemporal domain of the point cloud.

SOURCE [ISO 19123:2005].

NOTE 1 The data type EX_Extent, is defined in ISO 19103.  Extents may be specified in both space and time. 

NOTE 2 The domain extent shall be specified at least in space by using EX_BoundingPolygon, EX_GeographicBoundingBox or EX_GeographicDescription."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "extent"@en .

elevation-att:extremalUnscaledCoordinates a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "extremalUnscaledCoordinates"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range elevation:ExtremalUnscaledCoordinates ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """The max and min data fields are the actual unscaled extents of the LAS point file data, specified in the coordinate system of the LAS data.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "extremalUnscaledCoordinates"@en .

elevation-att:fieldOfView a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "fieldOfView"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Field of the view of the lidar sensor."@en ;
