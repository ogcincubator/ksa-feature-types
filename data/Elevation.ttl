    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Max flight height expressed in reference to mean sea level."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "maxFlyingHeightMSL"@en .

elevation-att:minFlyingHeightMSL a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "minFlyingHeightMSL"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Min flight height expressed in reference to mean sea level."@en ;
