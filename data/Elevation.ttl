@prefix ksa-ft: <https://w3id.org/ksa/feature-types/> .
@prefix ogc-ft: <http://www.opengis.net/def/metamodel/featuretypes/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ksa-ft:BathymetricElevationGridCoverageAggregation a owl:Class,
        skos:Concept ;
    rdfs:label "Elevation Grid Coverage Aggregation"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition "Geometrical characteristics of the elevation grid coverage aggregation."@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:BathymetricSpotElevationClassValue a owl:Class,
        skos:Concept ;
    rdfs:label "BathymetricSpotElevationClassValue"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition """Possible classification values for bathymetric spot elevations based on Topo-Bathy LAS Domain Profile.

NOTE These classes are proposed regardless of the acquisition method of the spot elevation. They correspond to the Topo-Bathy LAS Domain Profile point classes."""@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:BreakLineTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Break Line Type"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition "List of possible type values for break lines based on the physical characteristics of the break line [in the elevation surface]."@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:ContourLineTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Contour Line Type"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition "List of possible categories of contour lines based on the equidistance parameter of the data set."@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:DepthMeasurementQualityValue a owl:Class,
        skos:Concept ;
    rdfs:label "Depth Mesurement Quality"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition """Quality of depth measurement conformant with IHO s-57. 

NOTE S-57 acronym is QUASOU."""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/CRS> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "CRS" ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:CRS ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> .

ksa-ft:LandElevationGridCoverageAggregation a owl:Class,
        skos:Concept ;
    rdfs:label "Elevation Grid Coverage Aggregation"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition "Geometrical characteristics of the elevation grid coverage aggregation."@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:LandSpotElevationClassValue a owl:Class,
        skos:Concept ;
    rdfs:label "Spot Elevation Class"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition """Possible classification values for land spot elevations based on the LAS specification maintained by the American Society for Photogrammetry and Remote Sensing (ASPRS).

NOTE These classes are proposed regardless of the acquisition method of the spot elevation. They correspond to the ASPRS Standard LIDAR Point Classes."""@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:SoundingExpositionValue a owl:Class,
        skos:Concept ;
    rdfs:label "Sounding Exposition Value"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition """Sounding exposition conformant with IHO s-57. 

NOTE S-57 acronym is EXPSOU."""@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:SoundingStatusValue a owl:Class,
        skos:Concept ;
    rdfs:label "SoundingStatus"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition """Sounding status conformant with IHO S-101. 

NOTE S-57 acronym is STATUS."""@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:SpotElevationTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Spot Elevation Type"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition "Possible values for spot elevation points that describe a singularity of the surface."@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:SurveyTypeValue a owl:Class,
        skos:Concept ;
    rdfs:label "Survey Type"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition """Type of the survey conformant with IHO S-101. 

NOTE S-101 acronym is SURTYP."""@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:TechniqueOfSoundingMeasurementValue a owl:Class,
        skos:Concept ;
    rdfs:label "TechniqueOfSoundingMeasurement"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition """Technique of sounding measurement corresponding to IHO s-57 and IHO S-101.

NOTE S-57 and S-101 acronym is TECSOU."""@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:AcquisitionMethodValue a owl:Class,
        skos:Concept ;
    rdfs:label "Acquisition Method"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition """List of methods of acquisition of the points in the point cloud.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:BathymetricElevationGridCoverage a owl:Class,
        skos:Concept ;
    rdfs:label "Bathymetric Elevation Grid Coverage"@en ;
    ogc-ft:hasProperty <https://w3id.org/ksa/feature-types/Elevation/attribute/localDepthDatum> ;
    rdfs:subClassOf ksa-ft:ElevationGridCoverage ;
    skos:broader ksa-ft:ElevationGridCoverage ;
    skos:definition "Elevation grid for bathymetric elevation.  The value of the grid cells is depth which is measured upwards."@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:CRS a owl:Class,
        skos:Concept ;
    rdfs:label "CRS"@en ;
    skos:definition "coordinate reference system which is usually single but may be compound"@en .

ksa-ft:CRSRepresentationInLASFileValue a owl:Class,
        skos:Concept ;
    rdfs:label "CRS Representation In LAS File"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition """Representation of the Coordinate Reference System as it is in the LAS file.

NOTE 1 In LAS files versions 1.0-1.3 the CRS was represented as GeoTIFF. In version 1.4 the CRS can be represent as GeoTIFF or WKT (Well Known Text).

NOTE 2 Point Record Formats 0-5 can use either GeoTIFF of WKT. Point Record Formats 6-10 must use WKT.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:ChartDatum a owl:Class,
        skos:Concept ;
    rdfs:label "Chart Datum"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition "Local vertical coordinate reference system (Coordinate Reference System) which is used to refer and portray depth measurements as property values. This data type is intended to describe the main characteristics of a vertical Coordinate Reference System for measurement of depths in cases where this information is rarely or not available through a Coordinate Reference System registry (for example, like EPSG). Examples of a local vertical Coordinate Reference System include local datums for referring depths in lakes and navigable rivers."@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:CoordinateComputationalData a owl:Class,
        skos:Concept ;
    rdfs:label "Coordinate Computational Data"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition "Encapsulation of data used to compute points coordinates in specified Coordinate Reference System when points are given in raw form of integers only.  For further explanation see ASPRS LAS file specification 1.4 - R15."@en ;
    skos:inScheme ksa-ft:Elevation .

ksa-ft:DomainProfile a owl:Class,
        skos:Concept ;
    rdfs:label "Domain Profile"@en ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/codeLists> ;
    skos:definition "Domain profile of LAS file."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/CRSRepresentation> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "CRSRepresentation"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:CRSRepresentationInLASFileValue ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Representation of the Coordinate Reference System as it is in the LAS file.

NOTE 1 In LAS files versions 1.0-1.3 the CRS was represented as GeoTIFF. In version 1.4 the CRS can be represent as GeoTIFF or WKT (Well Known Text).

NOTE 2 Point Record Formats 0-5 can use either GeoTIFF of WKT. Point Record Formats 6-10 must use WKT."""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/GPSTimeType> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "GPSTimeType"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:GPSTimeTypeValue ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "The meaning of GPS Time in the point records."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/LASFile> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "LASFile"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Identifier of LAS file contaninig data for this point cloud."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/LASFileSpecificationVersion> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "LASFileSpecificationVersion"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Version of the LAS file specification.

EXAMPLE 1: 1.2

EXAMPLE 2: 1.4"""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/WDPFile> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "WDPFile"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "WDP file contaninig asociated with LAS file."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/WDPStorageType> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "WDPStorageType"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:WDPStorageTypeValue ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Type of storage of Waveform Data Package.

EXAMPLE 1 Internal in the LAS file (deprecated in LAS 1.4)

EXAMPLE 2 External in the separate (to LAS file) WDP file that has the same name as corresponding LAS file."""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/acquisitionMethod> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "acquisitionMethod"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:AcquisitionMethodValue ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Method of acquisition of the points in the point cloud."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/averageAltitude> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "averageAltitude"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Average flying altitude above the ground."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/averagePositionalAccuracy> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "averagePositionalAccuracy"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Average positional accuracy."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/averageVerticalAccuracy> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "averageVerticalAccuracy"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Average vertical accuracy of points within point cloud."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/coordinateComputationalData> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "coordinateComputationalData"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:CoordinateComputationalData ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Encapsulation of data used to compute points coordinates in specified CRS when points are given in raw form of integers only.

NOTE For further explanation see ASPRS LAS file specification 1.4 - R15."""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/domainProfile> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "domainProfile"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:DomainProfile ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Domain profile of LAS file associated with this point cloud."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/extent> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "extent"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Extent of the spatiotemporal domain of the point cloud.

SOURCE [ISO 19123:2005].

NOTE 1 The data type EX_Extent, is defined in ISO 19103.  Extents may be specified in both space and time. 

NOTE 2 The domain extent shall be specified at least in space by using EX_BoundingPolygon, EX_GeographicBoundingBox or EX_GeographicDescription."""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/extremalUnscaledCoordinates> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "extremalUnscaledCoordinates"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:ExtremalUnscaledCoordinates ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """The max and min data fields are the actual unscaled extents of the LAS point file data, specified in the coordinate system of the LAS data.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/fieldOfView> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "fieldOfView"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Field of the view of the lidar sensor."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/fileCreationDate> a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "fileCreationDate"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range xsd:date ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Date on which this file was created."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/footprint> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "footprint"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Footprint of the area covered by lidar data.

NOTE 1 It shall be convex hull of the projection od the point cloud onto the CRS.

NOTE 2 The CRS shall be the same as CRS of point cloud."""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/generatingSoftware> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "generatingSoftware"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:GeneratingSoftwareValue ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Specification which generating software package and version was used during LAS file creation."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/geometries> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "geometries"@en ;
    rdfs:domain ksa-ft:ElevationTIN ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Represents the collection of geometric properties of the elevation TIN spatial object."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/horizontalAccuracy> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "horizontalAccuracy"@en ;
    rdfs:domain ksa-ft:ElevationVectorObject ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Horizontal accurracy."@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/laserLateral> a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "laserLateral"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range xsd:integer ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Laser lateral in percents.

EXAMPLE 20%"""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/laserPower> a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "laserPower"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range xsd:integer ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Laser power in percents.

EXAMPLE 100%"""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/laserPulseRate> a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "laserPulseRate"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range xsd:integer ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Laser pulse rate.

EXAMPLE: 700 kHz"""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/lineScanningSpeed> a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "lineScanningSpeed"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range xsd:integer ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Line scanning speed (lines per second).

Example 150 lps"""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/localDepthDatum> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "localDepthDatum"@en ;
    rdfs:domain ksa-ft:BathymetricElevationGridCoverage ;
    rdfs:range ksa-ft:ChartDatum ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition """Identification of a local vertical coordinate reference system not included in a registry, which is used to refer depth measurements.

NOTE This data type is applicable only to bathymetric vector objects.

EXAMPLE Local datums for referring depths in lakes and navigable rivers."""@en ;
    skos:inScheme ksa-ft:Elevation .

<https://w3id.org/ksa/feature-types/Elevation/attribute/maxFlyingHeightMSL> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "maxFlyingHeightMSL"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:broader <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
    skos:definition "Max flight height expressed in reference to mean sea level."@en ;
