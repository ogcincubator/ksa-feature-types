<https://w3id.org/ksa/feature-types/Elevation/attribute/waveformPacketDescriptor> a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "waveformPacketDescriptor"@en ;
    rdfs:domain ksa-ft:LASPointCloud ;
    rdfs:range ksa-ft:WaveformPacketDescriptor ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/Elevation/attribute> ;
