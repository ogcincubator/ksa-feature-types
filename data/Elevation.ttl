    rdfs:range xsd:integer ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Average amount of points measured per square meter."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "pointDensity"@en .

elevation-att:surfaceType a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "surfaceType"@en ;
    rdfs:domain elevation:ElevationTIN ;
    rdfs:range transport:SurfaceTypeValue ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Attribute indicating the type of elevation surface that the elevation TIN describes in relation to the Earth's bare surface."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "surfaceType"@en .

elevation-att:syntheticReturnNumbers a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "syntheticReturnNumbers"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range xsd:boolean ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Determination if return numbers associated with points are synthetically created.

NOTE If "true", the point return numbers in the point data records
have been synthetically generated. This could be the case, for
example, when a composite file is created by combining a First
Return File and a Last Return File, or when simulating return
numbers for a system not directly supporting multiple returns."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "syntheticReturnNumbers"@en .

elevation-att:systemIdentifier a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "systemIdentifier"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range elevation:SystemIdentifierValue ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Information about what hardware or what method were used to create points.

NOTE 1 The version 1.0 specification assumed that LAS files are exclusively generated as a result of collection by a hardware sensor. Subsequent versions recognize that files often result from extraction, merging, or modifying existing data files.

SOURCE ASPRS LAS Specification 1.4 - R15 (http://www.asprs.org/wp-content/uploads/2019/07/LAS_1_4_r15.pdf)"""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "systemIdentifier"@en .

elevation-att:userData a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "userData"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Optional text description of the data.

NOTE In this attribute can be stored custom information normally included in the LAS file in the variable length records (VLR) or extended variable length records (EVLR)."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "userData"@en .

elevation-att:verticalAccuracy a owl:ObjectProperty,
        skos:Concept ;
    rdfs:label "verticalAccuracy"@en ;
    rdfs:domain elevation:ElevationVectorObject ;
    rdfs:subClassOf elevation:attribute ;
