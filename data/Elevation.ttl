    rdfs:label "numberOfPoints"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range xsd:integer ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition "Number of points in this point cloud."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "numberOfPoints"@en .

elevation-att:numberOfPointsByReturn a owl:DatatypeProperty,
        skos:Concept ;
    rdfs:label "numberOfPointsByReturn"@en ;
    rdfs:domain elevation:LASPointCloud ;
    rdfs:range xsd:integer ;
    rdfs:subClassOf elevation:attribute ;
    skos:broader elevation:attribute ;
    skos:definition """Number of points by return.

NOTE The first value will be the total number of records from the first return, the second contains the total number for return two, and so on."""@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "numberOfPointsByReturn"@en .

elevation-att:pointDataRecordFormat a owl:ObjectProperty,
