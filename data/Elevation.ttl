        elevation-att:surveyAuthority,
        elevation-att:surveyType,
        elevation-att:techniqueOfSoundingMeasurement ;
    rdfs:subClassOf elevation:GenericSpotElevation ;
    skos:broader elevation:GenericSpotElevation ;
    skos:definition "A point feature which describes the elevation of an bathymetric Earth's surface at a specific location. It often represents a singular point in the morphology of a land or water body's floor surface. The attribute propertyType is equal to 'height' in the case of spot heights or to 'depth' in the case of spot depths."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Sounding"@en .

elevation:SoundingCollection a owl:Class,
        skos:Concept ;
    rdfs:label "Sounding Collection"@en ;
    ogc-ft:hasProperty elevation-att:description,
        elevation-att:domainExtent,
        elevation-att:exposition,
        elevation-att:information,
        elevation-att:localDepthDatum,
        elevation-att:maxScale,
        elevation-att:measurementQuality,
        elevation-att:minScale,
        elevation-att:name,
