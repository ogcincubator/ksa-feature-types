    ogc-ft:hasProperty elevation-att:localDepthDatum,
        elevation-att:phenomenonTime ;
    rdfs:subClassOf <https://w3id.org/ksa/feature-types/BaseTypes/SpatialDataSet> ;
    skos:broader <https://w3id.org/ksa/feature-types/BaseTypes/SpatialDataSet> ;
    skos:definition "Collection of Bathymetric Isolated Areas."@en ;
    skos:inScheme ksa-ft:Elevation ;
