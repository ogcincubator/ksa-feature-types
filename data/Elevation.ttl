        elevation-att:sourceIndication ;
    rdfs:subClassOf elevation:GenericContourLine ;
    skos:broader elevation:GenericContourLine ;
    skos:definition "A linear feature composed of a set of adjoining locations characterized by having the same elevation depth. It describes, together with other contour lines present in the area, the local morphology of the bathymetry surface."@en ;
    skos:inScheme ksa-ft:Elevation ;
    skos:prefLabel "Bathymetric Contour Line"@en .

elevation:DepthArea a owl:Class,
        skos:Concept ;
    rdfs:label "Depth Area"@en ;
    ogc-ft:hasProperty elevation-att:description,
        elevation-att:geographicalExtent,
